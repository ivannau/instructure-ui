  "bugs": "https://github.com/instructure/instructure-ui/issues",
  "scripts": {
    "prestart": "node buildScripts/build-docs.js",
    "start": "ui-build --bundle && ui-scripts --server -p 8001",
    "start-watch": "node buildScripts/build-docs.js && ui-build --bundle --watch -p 8080",
    "bundle": "node buildScripts/build-docs.js && ui-build --bundle",
    "lint": "ui-test --lint",
    "lint-fix": "ui-test --lint --fix",
    "clean": "ui-build --clean",
    "createStatsFile": "mkdirp ./__build__ && webpack --profile --json > __build__/bundleStats.json",
    "analyseStatsFile": "webpack-bundle-analyzer __build__/bundleStats.json"
  },
